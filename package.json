{
    "name": "mini-project",
    "version": "1.0.0",
    "devDependencies": {
        "http-server": "^14.1.1",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^5.0.0",
        "mochawesome-report-generator": "^6.2.0"
    },
    "dependencies": {
        "cypress": "^14.5.4"
    },
    "scripts": {
        "cy:run": "cypress run",
        "merge:reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/report.json",
        "generate:report": "marge cypress/reports/report.json -f final-report -o cypress/reports",
        "test": "npm run cy:run && npm run merge:reports && npm run generate:report"
    }
}
